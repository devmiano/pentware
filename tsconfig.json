{
	"compilerOptions": {
		"outDir": "/static/frontend/public/",
		"sourceMap": true,
		"strictNullChecks": true,
		"noResolve": false,
		"noImplicitAny": false,
		"module": "commonjs",
		"target": "es6",
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
		"lib": ["es6", "dom"],
		"types": ["node", "react", "react-dom"]
	},
	"exclude": ["./node_modules/**/*"]
}
